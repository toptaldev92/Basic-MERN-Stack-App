{
    "name": "basic-mern-app",
    "version": "1.0.0",
    "description": "A Basic MERN Stack App just for practice",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "client-server": "npm start --prefix client",
        "client": "cd client && npm start",
        "server": "nodemon app.js",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
